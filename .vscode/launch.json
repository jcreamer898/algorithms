    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": ["<node_internals>/**"],
            "protocol": "inspector",
            "console": "internalConsole",
            "program": "${workspaceFolder}/src/problems/maxSub",
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
            "runtimeArgs": ["-x", ".ts", "--nolazy"],
            "sourceMaps": true,
            "port": 9229
        }
    ]
}
